<%= form_with(model: @questions, html: {multipart: true} ,local: true) do |form| %>

  <div class="field">
    <%= form.label :question, "Images" %>
    <%= form.cl_image_upload :question %>
  </div>

  <div class="field">
    <%= form.label :answer %>
    <%= form.text_field :answer %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>
