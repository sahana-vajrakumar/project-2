<%= form_with(model: @questions, local: true) do |form| %>

  <div class="field">
    <%= form.label :question, "Images" %>
    <%= form.text_field :question %>
  </div>

  <div class="field">
    <%= form.label :answer %>
    <%= form.text_field :answer %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>
